<!doctype html>
<html class="" lang="en">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title><%= content_for?(:title) ? yield(:title) + ' - Portfolio' : 'Portfolio' %></title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0">
  <%= stylesheet_link_tag    'application/application' %>
  <%= javascript_include_tag 'head/application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="body">
    <header>
      <%= render 'header' %>
    </header>

    <div id="container">
      <%= render 'navigation' %>
      <%= render 'flashes' %>
      <div id="main" role="main">
        <%= yield %>
      </div>
    </div>
  </div>
  <%= render 'footer' %>
  <%= javascript_include_tag 'application/application' %>
  <%= yield :javascripts %>
</body>
</html>